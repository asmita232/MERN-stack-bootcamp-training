{
  "compilerOptions": {
    "target": "es5",
    "outDir": "./public/js",                        
    "strict": true,
    "esModuleInterop": true, 
    "module": "ES2015",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}